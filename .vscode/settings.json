{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#09160d",
    "activityBar.background": "#20102c",
    "activityBar.foreground": "#ffffff",
    "activityBar.inactiveForeground": "#ffffff79",
    "activityBarBadge.background": "#6c44ca",
    "activityBarBadge.foreground": "#ededed",
    "commandCenter.border": "#ffffff9e",
    "sash.hoverBorder": "#7f3fa1",
    "statusBar.background": "#20102c",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#656565a9",
    "statusBarItem.remoteBackground": "#6c44ca",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#20102c",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#2c1c4c",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "code-runner.customCommand": "",
  "code-runner.executorMap": {
    "python": "python $internalDir"
  },
  "code-runner.runInTerminal": true,
  "code-runner.enableAppInsights": false,
  "code-runner.fileDirectoryAsCwd": false,
  "code-runner.clearPreviousOutput": false,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.temporaryFileName": "tempCode",
  "code-runner.ignoreSelection": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "gitlens.codeLens.symbolScopes": ["!Module"],
  "editor.formatOnType": true,
  "editor.wordBasedSuggestions": "off",
  "editor.defaultFormatter": "ms-python.autopep8",
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "autopep8.args": [
    "--ignore=E731 "
  ]
}
